{
  "name": "bi",
  "productName": "desktop-app",
  "version": "1.0.0",
  "private": true,
  "description": "Bookmark intelligence desktop app",
  "main": "./dist/main/main.js",
  "scripts": {
    "tsc": "node ./scripts/tsc.js",
    "webapp": "bash ../../scripts/build-web-app-to-desktop-app.sh",
    "electron:start": "electron-forge start",
    "dev:build": "npm run webapp && npm run dev",
    "dev": "cross-env WATCH=true && rimraf ./dist && npm run tsc && cp -rf ./src/web-app ./dist/ && npm run electron:start",
    "build": "bash ../../scripts/build-desktop-app.sh",
    "app:package": "npm run build && electron-forge package",
    "app:make": "npm run build && electron-forge make",
    "app:publish": "npm run build && electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "neo"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.5",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/plugin-webpack": "^6.0.4",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "^6.0.0",
    "electron": "22.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "cross-env": "7.0.3",
    "rimraf": "4.1.2",
    "typescript": "^4.9.5",
    "@types/lodash": "4.14.191",
    "@types/uuid": "9.0.0",
    "@types/detect-port": "1.3.2",
    "@types/cross-zip": "4.0.0",
    "cross-zip": "4.0.0",
    "glob": "9.0.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-devtools-installer": "3.2.0",
    "electron-default-menu": "1.0.2",
    "lodash": "4.17.21",
    "uuid": "9.0.0",
    "electron-log": "5.0.0-beta.16",
    "fs-extra": "11.1.0"
  },
  "envars": {
    "cwd": "../../env"
  }
}
