{
  "name": "seekbot",
  "productName": "SeekBot",
  "version": "1.0.0",
  "private": true,
  "description": "SeekBot desktop app",
  "main": "./dist/main/index.js",
  "scripts": {
    "tsc": "node ./scripts/tsc.js",
    "electron:start": "electron-forge start",
    "dev": "npm run build:webapp && npm run dev:watch",
    "dev:ui": "vite serve --port 7777",
    "dev:watch": "cross-env WATCH=true && bash ../../scripts/copy-libs.sh && rimraf ./dist && npm run tsc && cp -rf ./src/web-app ./dist/ && cp -rf ./src/ui ./dist/ && dotenv -e ../../env/.env -c -- npm run electron:start",
    "build": "bash ../../scripts/build-desktop-app.sh",
    "build:ui": "tsc --project ./tsconfig-ui.json && vite build",
    "build:webapp": "bash ../../scripts/build-web-app-to-desktop-app.sh",
    "app:package": "npm run build && electron-forge package",
    "app:make": "npm run build && electron-forge make --platform=darwin --verbose",
    "app:publish": "npm run build && electron-forge publish"
  },
  "keywords": [],
  "author": {
    "name": "neo"
  },
  "license": "GPL-3.0",
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron/osx-sign": "^1.0.5",
    "electron": "27.0.4",
    "node-loader": "^2.0.0",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "cross-env": "7.0.3",
    "rimraf": "5.0.5",
    "typescript": "^5.2.2",
    "@types/lodash": "4.14.201",
    "@types/uuid": "9.0.7",
    "@types/detect-port": "1.3.5",
    "@types/cross-zip": "4.0.2",
    "cross-zip": "4.0.0",
    "glob": "10.3.10",
    "vite": "4.5.0",
    "@playwright/test": "^1.39.0",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.1.1",
    "autoprefixer": "^10.4.16",
    "electron-builder": "^24.6.4",
    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vite-plugin-electron": "^0.15.4",
    "vite-plugin-electron-renderer": "^0.14.5",
    "dotenv-cli": "7.3.0"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-devtools-installer": "3.2.0",
    "electron-default-menu": "1.0.2",
    "electron-updater": "^6.1.4",
    "lodash": "4.17.21",
    "uuid": "9.0.1",
    "electron-log": "5.0.0",
    "fs-extra": "11.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.18.0",
    "graphql": "^16.8.1",
    "@apollo/client": "^3.8.7",
    "i18next": "^23.7.3",
    "react-i18next": "^13.4.0",
    "antd": "^5.11.1",
    "@ant-design/pro-components": "^2.6.48",
    "envalid": "^7.3.1",
    "dotenv": "16.3.1"
  },
  "envars": {
    "cwd": "../../env"
  }
}
