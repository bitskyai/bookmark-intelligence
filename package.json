{
  "name": "bookmark-intelligence",
  "displayName": "Bookmark intelligence",
  "version": "0.1.0",
  "description": "Automate capture bookmark website screenshots, save bookmark website content, and easy-to-find bookmarks with powerful search engine",
  "repository": "https://github.com/bitskyai/smart-bookmarks.git",
  "author": "help.bitskyai@gmail.com",
  "license": "GPL-3.0",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "scripts",
      "projects/browser-extension",
      "projects/ui",
      "projects/shared"
    ]
  },
  "engines": {
    "node": ">=16.17.1",
    "yarn": ">= 1.0.0"
  },
  "scripts": {
    "dev": "",
    "copy-meilisearch-to-api": "bash ./scripts/copy-meilisearch-to-api.sh",
    "build:ui-to-api": "bash ./scripts/build-ui-to-api.sh",
    "build:web-app-to-desktop-app": "bash ./scripts/build-web-app-to-desktop-app.sh",
    "build:web-app": "bash ./scripts/build-web-app.sh",
    "build:desktop-app": "bash ./scripts/build-desktop-app.sh",
    "release:desktop-app": "bash ./scripts/release-desktop-app.sh",
    "release:browser-extension": "bash ./scripts/release-browser-extension.sh",
    "test": "jest",
    "lint": "eslint --cache --report-unused-disable-directives .",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "setup": "zx ./scripts/setup.js",
    "syncpack:fix": "syncpack fix-mismatches  -d -p -P",
    "syncpack:list": "syncpack list-mismatches",
    "prepare": "husky install"
  },
  "dependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/core": "^7.18.2",
    "@babel/node": "^7.17.10",
    "@babel/plugin-proposal-class-properties": "^7.17.12",
    "@babel/plugin-proposal-object-rest-spread": "^7.18.0",
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.17.12",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@emotion/babel-plugin": "^11.9.2",
    "@emotion/eslint-plugin": "^11.7.0",
    "@emotion/react": "^11.9.0",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.50.0",
    "babel-jest": "^28.1.0",
    "babel-plugin-import": "^1.13.5",
    "babel-plugin-relay": "^13.2.0",
    "chalk": "^5.0.1",
    "cross-spawn": "^7.0.3",
    "dotenv": "16.0.3",
    "dotenv-cli": "7.1.0",
    "eslint": "8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.30.0",
    "eslint-plugin-react-hooks": "^4.5.0",
    "graphql-config": "^4.3.1",
    "husky": "^8.0.1",
    "inquirer": "^8.2.4",
    "jest": "^28.1.0",
    "minimist": "^1.2.6",
    "prettier": "2.8.3",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "typescript": "^4.9.5",
    "@jest/types": "^28.1.0",
    "@types/babel__core": "^7.1.19",
    "@types/cross-spawn": "^6.0.2",
    "@types/inquirer": "^8.2.1",
    "@types/jest": "29.2.4",
    "@types/minimist": "^1.2.2",
    "@types/node": "^18.11.19",
    "syncpack": "^8.4.11",
    "husky": "^8.0.0"
  }
}
