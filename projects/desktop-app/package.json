{
  "name": "bi",
  "productName": "desktop-app",
  "version": "1.0.0",
  "private": true,
  "description": "Bookmark intelligence desktop app",
  "main": "./dist/main/main.js",
  "scripts": {
    "tsc": "node ./scripts/tsc.js",
    "webapp": "sh ./scripts/build-copy-web-app.sh",
    "dev": "cross-env WATCH=true && rimraf ./dist && npm run webapp && npm run tsc && electron-forge start",
    "electron:start": "electron-forge start",
    "installProdNodeModules": "cp package.json ./dist && cp yarn.lock ./dist && cd ./dist && yarn --prod && cd ..",
    "copy-make-files": "node ./scripts/generateZip.js",
    "app:package": "npm run webapp && npm run installProdNodeModules && electron-forge package",
    "app:make": "npm run webapp && npm run installProdNodeModules && electron-forge make && npm run copy-make-files",
    "app:publish": "npm run webapp && npm run installProdNodeModules && electron-forge publish",
    "lint": "eslint --ext .ts,.tsx ."
  },
  "keywords": [],
  "author": {
    "name": "neo",
    "email": "shaokexu@example.com"
  },
  "license": "MIT",
  "devDependencies": {
    "@electron-forge/cli": "^6.0.4",
    "@electron-forge/maker-deb": "^6.0.4",
    "@electron-forge/maker-rpm": "^6.0.4",
    "@electron-forge/maker-squirrel": "^6.0.4",
    "@electron-forge/maker-zip": "^6.0.4",
    "@electron-forge/plugin-webpack": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "5.47.0",
    "@typescript-eslint/parser": "5.50.0",
    "@vercel/webpack-asset-relocator-loader": "1.7.3",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "^6.0.0",
    "electron": "22.2.0",
    "eslint": "8.30.0",
    "eslint-plugin-import": "2.26.0",
    "fork-ts-checker-webpack-plugin": "^7.2.13",
    "node-loader": "^2.0.0",
    "style-loader": "^3.0.0",
    "ts-loader": "^9.2.2",
    "ts-node": "^10.0.0",
    "cross-env": "7.0.3",
    "rimraf": "4.1.2",
    "typescript": "^4.9.5",
    "@types/lodash": "4.14.191",
    "@types/uuid": "9.0.0",
    "@types/detect-port": "1.3.2",
    "@types/cross-zip": "4.0.0",
    "cross-zip": "4.0.0",
    "glob": "9.0.1"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "electron-devtools-installer": "3.2.0",
    "electron-default-menu": "1.0.2",
    "lodash": "4.17.21",
    "uuid": "9.0.0",
    "electron-log": "5.0.0-beta.16",
    "detect-port": "1.5.1"
  },
  "envars": {
    "cwd": "../../env"
  }
}
